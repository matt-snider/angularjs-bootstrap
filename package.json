{
  "name": "angularjs-bootstrap",
  "version": "0.0.1",
  "description": "AngularJS components for Bootstrap 4",
  "main": "dist/lib/angularjs-bootstrap.js",
  "scripts": {
    "lib:build": "rimraf dist/lib && webpack --env dev && webpack --env build",
    "start": "npm run docs:server",
    "docs:build": "rimraf dist/docs && webpack --env build --bail --progress --profile",
    "docs:server": "webpack-dev-server --history-api-fallback --inline --progress",
    "test": "karma start",
    "test-watch": "karma start --auto-watch --no-single-run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/matt-snider/angularjs-bootstrap.git"
  },
  "keywords": [
    "webpack",
    "es6",
    "starter",
    "library",
    "universal",
    "umd",
    "commonjs"
  ],
  "files": [
    "dist/lib"
  ],
  "author": "Matt Snider",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/matt-snider/angularjs-bootstrap/issues"
  },
  "homepage": "https://github.com/matt-snider/angularjs-bootstrap",
  "peerDependencies": {
    "angular": ">=1.5.0",
    "angular-animate": ">=1.5.0",
    "angular-messages": ">=1.5.0",
    "octicons": "*"
  },
  "devDependencies": {
    "@uirouter/angularjs": "1.0.15",
    "add": "2.0.6",
    "angular": "1.6.10",
    "angular-animate": "1.6.6",
    "angular-messages": "1.6.10",
    "angular-mocks": "1.6.10",
    "autoprefixer": "6.7.7",
    "babel-cli": "6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "8.2.3",
    "babel-loader": "7.1.2",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-angularjs-annotate": "0.8.2",
    "babel-preset-env": "1.7.0",
    "base-href-webpack-plugin": "2.0.0",
    "bootstrap": "4.0.0-beta",
    "common-tags": "1.7.2",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "^3.0.0",
    "eslint": "^5.16.0",
    "eslint-loader": "2.0.0",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "3.2.0",
    "js-yaml": "^3.13.1",
    "mini-css-extract-plugin": "^0.4.0",
    "node-libs-browser": "2.0.0",
    "octicons": "^6.0.1",
    "prismjs": "1.14.0",
    "raw-loader": "0.5.1",
    "rimraf": "2.6.2",
    "style-loader": "0.13.2",
    "url-loader": "1.0.1",
    "webpack": "4.7.0",
    "webpack-cli": "2.1.3",
    "webpack-dev-server": "3.1.14",
    "yargs": "10.0.3"
  }
}
